for i in range(int(input())):
    num = input()
    first = str(int(num) // 1000)
    second = str(int(num) % 1000)
    sum1 = 0
    sum2 = 0

    for i in first:
        sum1 += int(i)

    for j in second:
        sum2 += int(j)

    if sum1 == sum2:
        print("YES")

    else:
        print("NO")
